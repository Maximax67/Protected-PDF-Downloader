{
    "manifest_version": 3,
    "name": "Protected PDF downloader",
    "description": "Extension downloads protected PDFs from google drive. Created by Belikov Maxim.",
    "version": "1.2.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    }
}
